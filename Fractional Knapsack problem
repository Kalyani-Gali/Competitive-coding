#include <bits/stdc++.h>
using namespace std;
class item
{
public:
    double wt, val;
};
// sorting array of items by value per unit weight
bool compare(item A, item B)
{
    return A.val / A.wt > B.val / B.wt;
}
int main()
{
    int n;
    double c, mxValue = 0;
    cin >> n >> c;
    item arr[n];

    for (int i = 0; i < n; i++)
        cin >> arr[i].wt;

    for (int i = 0; i < n; i++)
        cin >> arr[i].val;

    sort(arr, arr + n, compare);

    for (int i = 0; i < n && c > 0.0; i++)
    {
        double x = min(arr[i].wt, c);
        /*  
            x is the weight or portion of item we can take
            it can be either c(ie capacity left) or itâ€™s own weight ie arr[i].wt
        */
        c -= x; //Decreasing capacity

        mxValue += (x * (arr[i].val / arr[i].wt));
    }

    cout << mxValue << endl;
    return 0;
}
